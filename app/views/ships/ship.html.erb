<div class="container card">
  <%= render 'shared/dashboard' %>
  <h1 class="font-custom-title">Navire LVL: <%= @player.ship_level %></h1>
  <div class="row">
    <div class="col-6">
      <div class="card m-3">
  <% if @player.ship_level == 0 %>
    Vous ne possédez pas de Navire
          <%=  image_tag "https://pbs.twimg.com/media/E4WNYNhXMAI48GA.jpg", :class => "product-photo mx-auto mt-3"  %>
          <div class="card-body text-center">
            <h5 class="card-title font-custom">
              Barque (Nécessaire pour vous rendre sur Grand Line)
            <br> <%= link_to "Acheter: 100.000 Berrys?", upgrade_ship_path(:cost => 100000),method: :patch,   class: "btn btn-success m-2 rounded-pill border border-success" %>
            </h5>
          </div>
        </div>
  <% elsif @player.ship_level == 1%>
    <%=  image_tag "http://cdn.shopify.com/s/files/1/0273/9529/3272/products/DrapeauxOnePieceBarbeNoire_1200x1200.png?v=1627667229", :class => "product-photo mx-auto mt-3"  %>
