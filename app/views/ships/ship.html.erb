              <% if @player.defeated_mob.include?('Capitaine Kuro')%>
                <br> <%= link_to "Acheter: 5 Millions de Berrys?", upgrade_ship_path(:cost => 5000000),method: :patch, class: "btn btn-success m-2 rounded-pill border border-success" %>
